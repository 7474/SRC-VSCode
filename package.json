{
    "name": "src-vscode",
    "displayName": "SRC",
    "description": "Add support to editing SRC data and eve.",
    "version": "0.0.4",
    "repository": "https://github.com/7474/SRC-VSCode",
    "publisher": "koudenpa",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "src-eve",
            "aliases": ["SRC-Event", "src-eve"],
            "extensions": [".eve"],
            "configuration": "./src-eve/language-configuration.json"
        },{
            "id": "src-data",
            "aliases": ["SRC-Data", "src-data"],
            "extensions": [".txt"],
            "configuration": "./src-data/language-configuration.json"
        }],
        "grammars": [{
            "language": "src-eve",
            "scopeName": "src.eve",
            "path": "./src-eve/syntaxes/src.tmLanguage.json"
        },{
            "language": "src-data",
            "scopeName": "src.txt",
            "path": "./src-data/syntaxes/src.tmLanguage.json"
        }]
    }
}
